
.page {
  height: 100%;
  width: 100%;
  background-color: white;

}

.page-scrollV {

  width: 100%;
  height: 200px;
}

.page-scrollV-Cell {
  height: 70px;
  width: 100%;
  display: flex;
  border: 0.5px solid #f6f6f6;
  flex-direction: row;
  align-items: center;
}

.scrollV-Item-first {
  
   border-bottom-color: #999999;
   
   align-self: center;

   margin-left: 15px;

}

.scrollV-Item-secend {
  
   border-bottom-color: #999999;
   margin-left: 15px;
   margin-top: 3px;

}

.scrollV-Item-three {
  
   border-bottom-color: #999999;
   margin-left: 30px;
   align-self: center;
   background-color: red;

}