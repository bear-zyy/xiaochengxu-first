
.cell {
  background: white;
  margin-bottom: 10px;
  display: flex;
  flex-direction: row;
  height: 80px;
  align-items: center;
}

.cell-image {
width: 60px;
height: 60px;
margin-left: 15px;
}

.cell-rightview {
height: 60px;
margin-left: 10px;
display: flex;
flex-direction: column;

}

.cell-rightview-top {
 display: flex;
 flex-direction: row;
 align-items: flex-end;
}

.cell-rightview-bottom {
  margin-top: 10px;
background: white;
display: flex;
flex-direction: row;
align-items: center;
}

.cell-title {
  color: #323232;
  font-size: 12px;

}

.cell-member-num{
  color: #323232;
  font-size: 10px;
}

.cell-rightview-leaderimage{
  margin-left:10px;
  width: 28px;
  height: 28px;
  border-radius: 50%;
}

.cell-imagelist{
 display: flex;
 flex-direction: row;
 align-items: center;
 height: 30px;
}

.cell-imagelist-view {
background: white;
width: 30px;
height: 30px;
display: flex;
flex-direction: row;
justify-content: center;
align-items: center;
border-radius: 50%;
}

.cell-imagelist-image{
  width: 28px;
  height: 28px;
  border-radius: 50%;
}