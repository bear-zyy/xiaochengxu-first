
page {
  display: flex;
  flex-direction: column; 
  width: 100%;
  height: 100%;
}

.page-topview{
  background: #2DABFF;
  height: 45px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;

}

.page-topview-but {
  color: white;
  font-size: 14px;

}

.page-scrollview {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 340pt;

}

.page-scrollview-baseview {
  display: flex;
  flex-direction: row;
  /* justify-content: start; */
  width: 200%;
  height: 340pt;
}

.page-scrollview-view {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 50%;

}

.page-scrollview-text {
    width: 750rpt;
  height: 450px;
}

.page-collection {

  margin-bottom: 10pt;

}

.page-collection-cell {
  width: 90pt;
  height: 100pt;
  margin-top: 10pt;
  margin-left: 30px;
}